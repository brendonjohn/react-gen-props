{
  "name": "react-gen-props",
  "version": "0.0.1",
  "description": "Use testcheck to generate props from your propTypes",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel src -d lib",
    "test": "mocha --compilers js:babel-register test/*.test.js",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "react",
    "testcheck",
    "quickcheck",
    "testing"
  ],
  "author": "Unbounce Marketing Solutions",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.13.1",
    "react": "15.1.0",
    "testcheck": "0.1.4"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "mocha": "2.5.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
